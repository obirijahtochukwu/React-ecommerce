 closeSidebar,\n        isSubmenuOpen,\n        openSubmenu,\n        closeSubmenu,\n        page,\n        location,\n      }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n};\n// make sure use\nexport const useGlobalContext = () => {\n  return useContext(AppContext);\n};\n\nexport { AppContext, AppProvider };\n","C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\NavBar\\Navigation.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\NavBar\\Navbar.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\NavBar\\Sidebar.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\NavBar\\Submenu.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Details.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Default.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Carousel.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Main.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\TopSellingItems.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\data2.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\Cart.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\CartColumns.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\CartItems.js",["106","107"],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\CartList.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\CartTotal.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\Cart\\EmptyCart.js",[],"C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\react-js\\src\\components\\PhoneGoods.js",["108"],"import React from 'react'\r\nimport { Link } from \"react-router-dom\";\r\n\r\n\r\nexport default function PhoneGoods() {\r\n return (\r\n  <div>\r\n   helllo phone\r\n  </div>\r\n )\r\n}\r\n","C:\\Users\\OBIRIJAH TOCHUKWU\\Desktop\\r